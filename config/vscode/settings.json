  // "git.autofetch": false,
  "workbench.colorCustomizations": {
    // // "editorCursor.foreground": "#ffff00",
    // "editorCursor.foreground": "#EBCB8B",
    "terminalCursor.foreground": "#ffff00",
    "[Gruvbox Dark Medium]": {
      // "sideBar.foreground": "#c7c7c7",
      "sideBar.background": "#242424",
      "sideBarSectionHeader.background": "#1E1E1E",
      "editor.background": "#1E1E1E",
      "breadcrumb.background": "#1E1E1E",
      "statusBar.background": "#1E1E1E",
      // "statusBar.foreground": "#626262",
      "minimap.background": "#1E1E1E",
      "titleBar.activeBackground": "#1E1E1E",
      "editorGroupHeader.tabsBackground": "#1E1E1E",
      "activityBar.background": "#1E1E1E",
      "tab.inactiveBackground": "#262626",
      "tab.activeBackground": "#333333",
      "tab.activeBorder": "#333333",
      // "editorLineNumber.foreground": "#282C34"
      "editorGutter.background": "#1E1E1E",
      "panel.background": "#1E1E1E",
      "terminal.background": "#1E1E1E"
      // "editor.lineHighlightBackground": "#ffffff",
      // "editor.lineHighlightBorder": "#0000ff"
    }
    // "[Nord]": {
    //   // "sideBar.foreground": "#c7c7c7",
    //   "sideBar.background": "#23262F",
    //   "sideBarSectionHeader.background": "#282C34",
    //   "editor.background": "#282C34",
    //   "breadcrumb.background": "#282C34",
    //   "statusBar.background": "#282C34",
    //   // "statusBar.foreground": "#626262",
    //   "minimap.background": "#282C34",
    //   "titleBar.activeBackground": "#282C34",
    //   "editorGroupHeader.tabsBackground": "#282C34",
    //   "activityBar.background": "#23262F",
    //   "tab.inactiveBackground": "#2E3440",
    //   "tab.activeBackground": "#5E81AC",
    //   "tab.activeBorder": "#282C34",
    //   // "editorLineNumber.foreground": "#282C34"
    //   "editorGutter.background": "#282C34",
    //   "panel.background": "#282C34",
    //   "terminal.background": "#282C34"
    //   // "editor.lineHighlightBackground": "#ffffff",
    //   // "editor.lineHighlightBorder": "#0000ff"
    // }
  },
  "editor.cursorBlinking": "expand",
  "workbench.settings.useSplitJSON": true,
  "extensions.ignoreRecommendations": true,
  // window
  // editor
  "diffEditor.wordWrap": "on",
  "editor.acceptSuggestionOnEnter": "off",
  "editor.renderLineHighlight": "all",
  // "editor.parameterHints.enabled": false,
  "files.trimFinalNewlines": true,
  "window.restoreWindows": "none",
  "editor.occurrencesHighlight": false,
  "glassit.alpha": 250,
  // tabs and spaces
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "[typescript]": {
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.tabSize": 2
  },
  "[css]": {
    "editor.tabSize": 2
  },
  "[scss]": {
    "editor.tabSize": 2
  },
  "[python]": {
    "editor.tabSize": 4
  },

  //tailwind
  "css.validate": false,
  "tailwindCSS.includeLanguages": {
    "html": "html",
    "javascript": "javascript",
    "css": "css"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  // emmet
  //emmet inside react jsx
  // "emmet.includeLanguages": {
  //     "javascript": "javascriptreact"
  // },
  // "emmet.excludeLanguages": [
  //     "markdown"
  // ],
  // vim
  // "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "security.workspace.trust.untrustedFiles": "open",
  // "terminal.external.linuxExec": "kitty",
  // autocmp off
  // "editor.quickSuggestionsDelay": 10000,
  // "editor.quickSuggestions": false,
  // "editor.suggest.showTypeParameters": false,
  "editor.hover.enabled": false,
  // "diffEditor.ignoreTrimWhitespace": false,
  // java
  // "java.completion.enabled": false,
  //"java.configuration.checkProjectSettingsExclusions": false,

  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "d"],
      "after": ["d", "d"]
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    }
  ],
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "explorer.confirmDragAndDrop": false,
  "oneDarkPro.vivid": false,

  // "[javascript]": {
  //   "editor.defaultFormatter": "esbenp.prettier-vscode"
  // },
  // Set the default
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  // Enable per-language
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.sideBar.location": "right",
  "glassit-linux.opacity": 95,
  "window.titleBarStyle": "custom",
  "files.autoSave": "afterDelay",
  "window.zoomLevel": 1,
  "workbench.startupEditor": "none"
}

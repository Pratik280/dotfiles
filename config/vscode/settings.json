{
  // "editor.fontFamily": "'Inconsolata  LGC Nerd Font', 'monospace', monospace, 'Droid Sans Fallback'ZZ",
  "editor.fontFamily": "'RobotoMono Nerd Font', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontSize": 13,
  "editor.lineHeight": 2,
  "update.showReleaseNotes": false,
  // minimal
  // "workbench.statusBar.visible": false,
  "workbench.activityBar.visible": false,
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": false,
  "window.menuBarVisibility": "toggle",
